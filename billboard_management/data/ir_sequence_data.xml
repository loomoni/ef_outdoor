<odoo>
    <data noupdate="1">
        <record id="seq_billboard_contract" model="ir.sequence">
            <field name="name">Billboard Contract</field>
            <field name="code">billboard.contract</field>
            <field name="prefix">CON/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_billboard_quotation" model="ir.sequence">
            <field name="name">Billboard Quotation</field>
            <field name="code">billboard.quotation</field>
            <field name="prefix">EF/%(y)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_tax_invoice" model="ir.sequence">
            <field name="name">Tax Invoice</field>
            <field name="code">tax.invoice</field>
            <field name="prefix">EF/%(year)s/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
